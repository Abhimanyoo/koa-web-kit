{
  "name": "koa-web-kit",
  "version": "2.6.2",
  "description": "A modern, production-ready, and full-stack node web framework",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENABLE_HMR=1 ENABLE_SSR=0 node app.js",
    "dev:ssr": "cross-env ENABLE_HMR=0 ENABLE_SSR=1 npm-run-all -p watch watch:ssr start",
    "dev:watch": "cross-env ENABLE_HMR=0 ENABLE_SSR=0 npm-run-all -p watch start",
    "start": "nodemon --trace-warnings app.js",
    "deploy": "./deploy.sh",
    "deploy:noinstall": "npm run deploy -- 1",
    "build:dev": "webpack --progress --hide-modules --config config/webpack.config.dev.js",
    "build": "npm run prod",
    "ssg": "cross-env STATIC_PREFIX='' APP_PREFIX='' npm run prod",
    "watch": "webpack --watch --progress --hide-modules --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "watch:ssr": "npm run ssr -- --watch",
    "ssr": "webpack --progress --config config/webpack.config.ssr.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "report": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "npm run build:dev && npm run build"
  },
  "engines": {
    "node": ">= 8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json,scss,html}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "react",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bootstrap": "4.1.3",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "date-fns": "1.29.0",
    "from2-string": "1.1.0",
    "get-port": "4.0.0",
    "koa": "2.6.2",
    "koa-body": "4.0.4",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-favicon": "2.0.1",
    "koa-helmet": "4.0.0",
    "koa-history-api-fallback": "0.1.3",
    "koa-morgan": "1.0.1",
    "koa-mount": "4.0.0",
    "koa-pino-logger": "2.1.3",
    "koa-router": "7.4.0",
    "koa-session": "5.10.0",
    "koa-static": "5.0.0",
    "lodash.isempty": "4.4.0",
    "make-dir": "1.3.0",
    "pino": "5.9.0",
    "pino-pretty": "2.2.4",
    "pm2": "3.2.2",
    "prop-types": "15.6.2",
    "qs": "6.6.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-loadable": "5.5.0",
    "react-router-dom": "4.3.1",
    "request": "2.88.0",
    "request-debug": "0.2.0",
    "slugify": "1.3.3",
    "url-parse": "1.4.4",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/helper-module-imports": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-jsx-source": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.5",
    "autoprefixer": "9.3.1",
    "babel-loader": "8.0.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "chai": "4.2.0",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "del": "3.0.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.0",
    "import-inspector": "2.0.0",
    "koa-webpack": "5.1.1",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.4.5",
    "mocha": "5.2.0",
    "node-sass": "4.10.0",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "preload-webpack-plugin": "3.0.0-beta.2",
    "prettier": "1.15.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.2",
    "webpack": "4.26.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "watch": [
      "api/",
      "config/",
      "build/node",
      "routes/",
      "utils/",
      "services/",
      "app-config.js",
      "app.js"
    ]
  }
}
