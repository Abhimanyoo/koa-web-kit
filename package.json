{
  "name": "koa-web-kit",
  "version": "2.5.5",
  "description": "A modern, production-ready, and full-stack node web framework",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENABLE_HMR=1 ENABLE_SSR=0 node app.js",
    "dev:ssr": "cross-env ENABLE_HMR=0 ENABLE_SSR=1 npm-run-all -p watch watch:ssr start",
    "dev:watch": "cross-env ENABLE_HMR=0 ENABLE_SSR=0 npm-run-all -p watch start",
    "start": "nodemon app.js",
    "deploy": "./deploy.sh",
    "deploy:noinstall": "npm run deploy -- 1",
    "build:dev": "webpack --progress --hide-modules --config config/webpack.config.dev.js",
    "build": "npm run prod",
    "ssg": "cross-env STATIC_PREFIX='' APP_PREFIX='' npm run prod",
    "build:analyze": "npm run prod:analyze",
    "watch": "webpack --watch --progress --hide-modules --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "watch:ssr": "npm run ssr -- --watch",
    "ssr": "webpack --progress --config config/webpack.config.ssr.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "prod:analyze": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "analyze": "npm run prod:analyze",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "npm run build:dev",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 8"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json,scss}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "react",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bootstrap": "4.1.3",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "from2-string": "1.1.0",
    "get-port": "4.0.0",
    "koa": "2.5.2",
    "koa-body": "4.0.4",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-favicon": "2.0.1",
    "koa-helmet": "4.0.0",
    "koa-history-api-fallback": "0.1.3",
    "koa-morgan": "1.0.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.2",
    "koa-static": "5.0.0",
    "lodash": "4.17.10",
    "lodash.isempty": "4.4.0",
    "make-dir": "1.3.0",
    "moment": "2.22.2",
    "pm2": "3.0.3",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-loadable": "5.5.0",
    "react-router-dom": "4.3.1",
    "request": "2.87.0",
    "request-debug": "0.2.0",
    "slugify": "1.3.1",
    "url-parse": "1.4.3",
    "whatwg-fetch": "2.0.4",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "9.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-import-inspector": "2.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "import-inspector": "2.0.0",
    "koa-webpack": "5.1.0",
    "lint-staged": "7.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "mocha": "5.2.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.9.3",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "postcss-loader": "3.0.0",
    "preload-webpack-plugin": "3.0.0-beta.2",
    "prettier": "1.14.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.1",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "watch": [
      "api/",
      "config/",
      "build/node",
      "routes/",
      "utils/",
      "services/"
    ]
  }
}
